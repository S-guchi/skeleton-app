{
  "expo": {
    "name": "{{APP_NAME}}",
    "slug": "skeleton-app",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "skeletonapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "{{APP_BUNDLE_ID}}",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "UIDeviceFamily": [
          1
        ],
        "NSPhotoLibraryUsageDescription": "ユーザーアイコンを設定するためにフォトライブラリにアクセスします。",
        "NSUserNotificationUsageDescription": "家事の記録を忘れないために通知を使用します。"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-localization",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6746ae89-5a22-4025-87ea-c7c1b93a0ca7"
      },
      "fullAppName": "{{APP_FULL_NAME}}"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/6746ae89-5a22-4025-87ea-c7c1b93a0ca7"
    }
  }
}
